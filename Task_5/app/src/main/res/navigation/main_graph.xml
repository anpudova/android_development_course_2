<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_graph"
    app:startDestination="@id/secondFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.task_5.fragments.FirstFragment"
        android:label="FirstFragment" >
        <action
            android:id="@+id/action_firstFragment_to_firstTwoFragment"
            app:destination="@id/firstTwoFragment"
            app:popUpTo="@id/firstFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/firstTreeFragment"
        android:name="com.example.task_5.fragments.FirstTreeFragment"
        android:label="FirstTreeFragment" />
    <fragment
        android:id="@+id/firstTwoFragment"
        android:name="com.example.task_5.fragments.FirstTwoFragment"
        android:label="FirstTwoFragment" >
        <action
            android:id="@+id/action_firstTwoFragment_to_firstTreeFragment"
            app:destination="@id/firstTreeFragment"
            app:popUpTo="@id/firstTwoFragment"
            app:popUpToInclusive="true"/>`
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.task_5.fragments.SecondFragment"
        android:label="SecondFragment" >
        <action
            android:id="@+id/action_secondFragment_to_secondTwoFragment"
            app:destination="@id/secondTwoFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondTwoFragment"
        android:name="com.example.task_5.fragments.SecondTwoFragment"
        android:label="SecondTwoFragment" />
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.task_5.fragments.ThirdFragment"
        android:label="ThirdFragment" >
        <action
            android:id="@+id/action_thirdFragment_to_thirdTwoFragment"
            app:destination="@id/thirdTwoFragment" />
    </fragment>
    <fragment
        android:id="@+id/thirdTreeFragment"
        android:name="com.example.task_5.fragments.ThirdTreeFragment"
        android:label="ThirdTreeFragment" />
    <fragment
        android:id="@+id/thirdTwoFragment"
        android:name="com.example.task_5.fragments.ThirdTwoFragment"
        android:label="ThirdTwoFragment" >
        <action
            android:id="@+id/action_thirdTwoFragment_to_thirdTreeFragment"
            app:destination="@id/thirdTreeFragment" />
    </fragment>
    <dialog
        android:id="@+id/bottomSheetFragment"
        android:name="com.example.task_5.fragments.BottomSheetFragment"
        android:label="BottomSheetFragment" >
        <action
            android:id="@+id/action_bottomSheetFragment_to_firstTreeFragment"
            app:destination="@id/firstTreeFragment"
            app:popUpTo="@id/bottomSheetFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_bottomSheetFragment_to_secondTwoFragment"
            app:destination="@id/secondTwoFragment" />
        <action
            android:id="@+id/action_bottomSheetFragment_to_thirdTwoFragment"
            app:destination="@id/thirdTwoFragment" />
    </dialog>
</navigation>